.top-Area {
  margin: 30rpx;
  justify-content: space-between;
  font-size: 40rpx;
  font-weight: bold;
}
.top-Area view {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.top-Area .top-Area-inputView {
  border-radius: 10rpx;
  padding: 0 20rpx;
  height: 80rpx;
}
.top-Area .top-Area-icon {
  width: 50rpx;
  height: 50rpx;
  margin: 0 30rpx;
}
.addIcon {
  position: fixed;
  bottom: 80px;
  right: 40px;
}
.addIcon image {
  width: 100rpx;
  height: 100rpx;
}
.teamPop {
  background-color: white;
  padding: 40rpx;
  width: 500rpx;
  border-radius: 20rpx;
  text-align: center;
}
.teamPop .teamPop-topRow {
  font-size: 36rpx;
  font-weight: bold;
  color: #2c2c2c;
  margin-bottom: 70rpx;
  position: relative;
  /* 设置相对定位 */
}
.teamPop .teamPop-topRow view {
  /* 绝对定位，4 个方向设置为 0 后，margin 设为 auto */
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.teamPop .teamPop-topRow .teamPop-topRow-button {
  position: absolute;
  right: 50rpx;
  margin: auto;
}
.teamPop .teamPop-topRow .teamPop-topRow-button view {
  width: 60rpx;
  font-size: 24rpx;
  font-size: normal;
}
.teamPop .teamPop-matePart {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-evenly;
  width: 100%;
}
.teamPop .teamPop-matePart .teamPop-matePart-part {
  margin-top: 20rpx;
  width: 120rpx;
}
.teamPop .teamPop-matePart .teamPop-matePart-part .teamPop-matePart-name {
  font-size: 26rpx;
}
